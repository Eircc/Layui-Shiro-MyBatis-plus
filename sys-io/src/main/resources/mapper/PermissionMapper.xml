<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccc.sys.io.mapper.PermissionMapper">

    <delete id="deleteRolePermissionByPid" parameterType="com.ccc.sys.io.domain.Permission" >
        DELETE FROM
            role_permission
        WHERE
            pid = ${id}

    </delete>
</mapper>